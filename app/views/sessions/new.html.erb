<div class="users-new-wrapper">
 <div class="container">
  <div class="row">
   <div class="col-md-offset-4 col-md-4 users-new-container">
     <%= form_for :session, url: login_path do |f| %>
     
     <div class="form-group">
       <%= f.label :email, class:'text-white'%>
       <%= f.email_field :email, class:'form-control' %>
     </div>
     
     <div class="form-group">
       <%= f.label :password, class:'text-white' %>
       <%= f.password_field :password, class:'form-control' %>
     </div>
       
       <%= f.submit "Log in", class: 'btn btn-white btn-block' %>
     <% end %>
     
       <p class="text-white">new user? <%= link_to("Sign Up", "users/new") %> </p>
   </div>
  </div>
 </div>
</div>
